<configuration>
  <appSettings>
    <add key="webpages:Version" value="3.0.0.0" />
    <add key="webpages:Enabled" value="false" />
    <add key="ClientValidationEnabled" value="true" />
    <add key="UnobtrusiveJavaScriptEnabled" value="true" />

    <!-- Authentication config -->
    <add key="ida:AADInstance" value="https://login.windows.net" />
    <add key="ida:RedirectUri" value="https://localhost:44303/" />
    <add key="ida:Tenant" value="%aadtenant%.onmicrosoft.com" />

    <!-- Authorization config -->
    <add key="ida:GraphResourceID" value="https://graph.windows.net" />
    <add key="ida:GraphApiVersion" value="2013-11-08" />
    <add key="ida:ClientId" value="%ClientId%" />
    <add key="ida:Password" value="%Key%" />
    <!-- The AAD group Id that represents the "Users" role. -->
    <add key="ida:UsersGroupId" value="%AADUsersGroupId%" />
    <!-- The AAD group Id that represents the "Admin" role. -->
    <add key="ida:AdminGroupId" value="%AADAdminGroupId%" />
  </appSettings>
  <system.web>
